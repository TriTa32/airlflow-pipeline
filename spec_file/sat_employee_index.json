{
    "type": "index_parallel",
    "spec": {
        "dataSchema": {
            "dataSource": "sat_employee",
            "timestampSpec": {
                "column": "load_datetime",
                "format": "iso"
            },
            "dimensionsSpec": {
                "dimensions": [
                    "sat_employee_hk",
                    "hub_employee_hk",
                    "load_datetime",
                    "record_source",
                    "hash_diff",
                    "employee_account",
                    "employee_name",
                    "employee_email",
                    "employee_location"
                ]
            },
            "granularitySpec": {
                "type": "uniform",
                "segmentGranularity": "DAY",
                "queryGranularity": "HOUR",
                "rollup": true
            }
        },
        "ioConfig": {
            "type": "index_parallel",
            "inputSource": {
                "type": "local",
                "baseDir": "/tmp",
                "filter": "employees_data_*.json"
            },
            "inputFormat": {
                "type": "json"
            }
        },
        "tuningConfig": {
            "type": "index_parallel",
            "partitionsSpec": {
                "type": "dynamic"
            }
        }
    }
}